{
  "extends": [
    "config:base",
    ":automergeDigest",
    ":automergeBranchPush",
    "github>aquaproj/aqua-renovate-config#1.2.5",
    "github>aquaproj/aqua-renovate-config:installer-script#1.2.5(install-aqua\\.sh)",
    "github>aquaproj/aqua-renovate-config:file#1.2.5(^aqua/.*\\.ya?ml$)"
  ],
  "platform": "github",
  "username": "repo-jeeves[bot]",
  "repositories": ["szinn/dotfiles"],
  "onboarding": false,
  "requireConfig": false,
  "gitAuthor": "repo-jeeves <106431701+repo-jeeves[bot]@users.noreply.github.com>",
  "timezone": "America/Toronto",
  "dependencyDashboardTitle": "Renovate Dashboard",
  "suppressNotifications": ["prIgnoreNotification"],
  "rebaseWhen": "conflicted",
  "commitBodyTable": true,
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "enabledManagers": ["regex"],
  "packageRules": [
    {
      "matchUpdateTypes": ["major"],
      "semanticCommitType": "feat!"
    },
    {
      "matchUpdateTypes": ["minor"],
      "semanticCommitType": "feat"
    },
    {
      "matchUpdateTypes": ["patch"],
      "semanticCommitType": "fix"
    },
    {
      "matchManagers": ["github-actions"],
      "automerge": true,
      "automergeType": "branch",
      "requiredStatusChecks": null,
      "matchUpdateTypes": ["minor", "patch"]
    },
    {
      "automerge": true,
      "automergeType": "branch",
      "requiredStatusChecks": null,
      "matchPackageNames": ["aquaproj/aqua-registry"]
    }
  ]
}
